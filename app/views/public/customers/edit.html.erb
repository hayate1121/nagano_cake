<h4>会員情報編集</h4>
<%= form_with model: @customer, url: customer_path do |f| %>
<ul>氏名</ul>
<%= f.text_field :last_name %>  <%= f.text_field :first_name %>
<ul>フリガナ</ul>
<%= f.text_field :last_name_kana %>  <%= f.text_field :first_name_kana %>
<ul>郵便番号</ul>
<%= f.text_field :postal_code %>
<ul>住所</ul>
<%= f.text_area :address %>
<ul>電話番号</ul>
<%= f.text_field :telephone_number %>
<ul>メールアドレス</ul>
<%= f.text_field :email %><br>
<%= f.submit "編集内容を保存" %>
<%= link_to "退会する", unsubscribe_path %>
<% end %>