<h4>会員一覧</h4>
<table class="table">
  <thead>
    <tr>
      <th>会員ID</th>
      <th>氏名</th>
      <th>メールアドレス</th>
      <th>ステータス</th>
    </tr>
  </thead>
  <% @customers.each do |customer| %>
  <tbody>
    <tr>
      <td>
        <%= customer.id %>
      </td>
      <td>
        <%= customer.name %>
      </td>
      <td>
        <%= customer.email %>
      </td>
      <td>
        <%= customer.is_deleted %>
      </td>
    </tr>
  </tbody>
  <% end %>
</table>